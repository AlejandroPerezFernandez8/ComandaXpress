	<queryString language="SQL">
		<![CDATA[SELECT *
FROM comandaxpress.ticket_detalle
	INNER JOIN comandaxpress.productos ON 
	 comandaxpress.ticket_detalle.producto_id = comandaxpress.productos.producto_id 
	INNER JOIN comandaxpress.categorias ON 
	 comandaxpress.productos.categoria_id = comandaxpress.categorias.categoria_id 
	INNER JOIN comandaxpress.tickets ON 
	 comandaxpress.ticket_detalle.ticket_id = comandaxpress.tickets.ticket_id 
	INNER JOIN comandaxpress.mesa ON 
	 comandaxpress.tickets.mesa_id = comandaxpress.mesa.mesa_id ,
	comandaxpress.usuarios]]>
	</queryString>
