	<subDataset name="MediaDeVentasPorCategoria" uuid="b27f086a-90bd-4f29-84e2-7ddf06f8d7b9">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="Adaptador"/>
		<queryString language="SQL">
			<![CDATA[SELECT
    c.nombre AS categoria_nombre,
    AVG(ingresos_totales) AS ingresos_medios
FROM (
    SELECT
        p.categoria_id,
        SUM(td.cantidad * p.precio) AS ingresos_totales
    FROM
        Ticket_Detalle td
    INNER JOIN
        Productos p ON td.producto_id = p.producto_id
    GROUP BY
        p.categoria_id, td.ticket_id
) subquery
INNER JOIN
    Categorias c ON subquery.categoria_id = c.categoria_id
GROUP BY
    c.nombre
ORDER BY
    ingresos_medios DESC]]>
		</queryString>
		<field name="categoria_nombre" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.name" value="nombre"/>
			<property name="com.jaspersoft.studio.field.label" value="categoria_nombre"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="categorias"/>
		</field>
