		<queryString language="SQL">
			<![CDATA[SELECT
    DATE_FORMAT(T.fecha_hora, '%Y-%m') AS Mes,
    COUNT(DISTINCT T.ticket_id) AS Numero_Tickets,
    SUM(TD.cantidad * P.precio) AS Ingreso_Total,
    ROUND(SUM(TD.cantidad * P.precio) / COUNT(DISTINCT T.ticket_id), 2) AS Precio_Medio
FROM
    Tickets T
JOIN
    Ticket_Detalle TD ON T.ticket_id = TD.ticket_id
JOIN
    Productos P ON TD.producto_id = P.producto_id
GROUP BY
    DATE_FORMAT(T.fecha_hora, '%Y-%m')
ORDER BY
    Mes]]>
