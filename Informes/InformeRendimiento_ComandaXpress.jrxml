		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="Adaptador"/>
		<queryString language="SQL">
			<![CDATA[SELECT
    m.numero AS numero_mesa,
    AVG(ticket_total.total_precio) AS precio_medio_ticket
FROM
    mesa m
JOIN
    Tickets t ON m.mesa_id = t.mesa_id
JOIN (
    SELECT
        td.ticket_id,
        SUM(p.precio * td.cantidad) AS total_precio
    FROM
        Ticket_Detalle td
    JOIN
        Productos p ON td.producto_id = p.producto_id
    GROUP BY
        td.ticket_id
) AS ticket_total ON t.ticket_id = ticket_total.ticket_id
GROUP BY
    m.numero]]>
		</queryString>
