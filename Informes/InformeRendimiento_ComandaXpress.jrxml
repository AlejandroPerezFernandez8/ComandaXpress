	<subDataset name="GraficoProductividadMesas" uuid="1aebf24f-c997-45b4-83e3-ea4f058f8e7d">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="Adaptador"/>
		<queryString language="SQL">
			<![CDATA[SELECT
    m.numero AS mesa_numero,
    SUM(td.cantidad * p.precio) AS ingresos_totales,
    ROUND((SUM(td.cantidad * p.precio) / (SELECT SUM(td.cantidad * p.precio)
                                          FROM Ticket_Detalle td
                                          INNER JOIN Productos p ON td.producto_id = p.producto_id)) * 100, 2) AS porcentaje_productividad
FROM
    Ticket_Detalle td
INNER JOIN
    Tickets t ON td.ticket_id = t.ticket_id
INNER JOIN
    mesa m ON t.mesa_id = m.mesa_id
INNER JOIN
    Productos p ON td.producto_id = p.producto_id
GROUP BY
    m.numero
ORDER BY
    porcentaje_productividad DESC]]>
		</queryString>
		<field name="mesa_numero" class="java.lang.Integer">
			<property name="com.jaspersoft.studio.field.name" value="numero"/>
			<property name="com.jaspersoft.studio.field.label" value="mesa_numero"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="mesa"/>
		</field>
		<field name="ingresos_totales" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.name" value="ingresos_totales"/>
			<property name="com.jaspersoft.studio.field.label" value="ingresos_totales"/>
		</field>
		<field name="porcentaje_productividad" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.name" value="porcentaje_productividad"/>
			<property name="com.jaspersoft.studio.field.label" value="porcentaje_productividad"/>
		</field>
